<%- include("headerUser") -%>
<body>
    <div class="box" id="heading">
    <h1>Group: <%=group.groupName %> </h1>
    </div>
    <div class="box">
    <h3>Subject: <%=group.groupSubject %> </h3>
    <h3>Group ID: <%=group._id %> </h3>
    <label>You can share this code with group members for them to join!</label>
    <h3>Users</h3>


    <% groupUsers.forEach(function(user)  { %>
    
        <% var userFirstName = user.firstName %> 
        <% var userLastName = user.lastName %> 
        <li><%=userFirstName + " " + userLastName%></li>
     <% }); %>

     <p></p>
     <% var newGroupId = groupId %> 
     <a href= <%="/teamCharter/" + newGroupId %>> <button>Team Charter</button></a>
     <a href= <%="/peerReview/" + newGroupId %>> <button>Peer Review</button></a>

    </div>

    
</body>
</html>